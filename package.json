{
  "name": "raktadan-backend",
  "version": "1.0.0",
  "description": "Backend API for Raktadan Blood Donation Platform",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "dev:docker": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up --build",
    "dev:docker:down": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml down",
    "build": "echo 'No build step needed for JavaScript'",
    "prod": "NODE_ENV=production node src/app.js",
    "seed:hospitals": "node src/seed/hospitals-simple.js",
    "seed:ambulances": "node src/seed/ambulances.js",
    "seed:camps": "node src/seed/donation-camps.js",
    "seed:districts": "node src/seed/districts.js",
    "seed:user": "node src/seed/test-user.js",
    "seed:all": "npm run seed:user && npm run seed:districts && npm run seed:hospitals && npm run seed:ambulances && npm run seed:camps",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "node prisma/seed.js",
    "prisma:deploy": "prisma migrate deploy",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "clean": "rm -rf node_modules package-lock.json && npm install"
  },
  "keywords": [
    "blood-donation",
    "nepal",
    "healthcare",
    "api"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "axios": "^1.13.4",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "express-validator": "^7.2.0",
    "firebase-admin": "^12.6.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.15",
    "pdfkit": "^0.15.0",
    "qrcode": "^1.5.4",
    "speakeasy": "^2.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.15.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.7.5",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.16",
    "@types/pdfkit": "^0.13.5",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "eslint": "^9.12.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "prisma": "^5.20.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
